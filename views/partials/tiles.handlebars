<div class="tiles">
  <div class="venue">
    <div class="favorites">
      <div class="favorites-icon">
        <a href="#"><img src="../../assets/img/icons-heart.png"></a>
      </div>
    </div>
    <a href="#popup1"><img src="{{image}}"></a>
  </div>
  <div class="tiles deck">
    <h3><a href="#popup1">{{title}}</a></h3>
    <p><img src="../../assets/img/5-star.png"><span class="reviews">{{totalRatings}} Reviews</span></p>
  </div>
  <div id="popup1" class="overlay">
	<div class="popup">
		<h2>{{title}}</h2>
    <p style="-webkit-margin-before: 0;"><img src="../../assets/img/5-star.png"><span class="reviews">{{totalRatings}} Reviews</span></p>
    <div class="popup-venue-image">
      <a href="{{link}}"><img src="{{image}}"></a>
    </div>
    <div class="popup-venue-deck">
      <p>{{address}}</p>
    </div>
		<a class="close" href="#">&times;</a>
	</div>
</div>
</div>
